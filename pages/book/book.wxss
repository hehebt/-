/* pages/book/book.wxss */
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    width: 100%;
}

.sub_container {
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #f5f5f5;
    margin-top: 100rpx;
    width: 100%;
}

.box {
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50px;
    background-color: #f5f5f5;
    height: 68rpx;
    width: 700rpx;
    color: #999999;
}
.box image {
    margin-right: 10px;
    width: 14px;
    height: 14px;
    margin-bottom: -2px;
}
.header {
    position: fixed;
    background-color: #fff;
    height: 100rpx;
    width: 100%;
    border-top: 1px solid #f5f5f5;
    border-bottom: 1px solid #f5f5f5;
    display: flex;
    align-items: center;
    justify-content: center;
    box-shadow: 0 0 3px 0 #e3e3e3;
    z-index: 888;
}

.head_img {
    width: 106rpx;
    height: 34rpx;
    margin-top: 40rpx;
}

.box_container {
    margin-top: 10rpx;
    display: flex;
    flex-wrap: wrap;
    padding: 0 90rpx;
    justify-content: space-between;
}